global
        log 127.0.0.1   local0
        log 127.0.0.1   local1 debug
        maxconn   45000 
        daemon
        nbproc      2
defaults
        timeout server 86400000
        timeout connect 86400000
        timeout client 86400000
        timeout queue   1000s

# [HTTP Site Configuration]
listen  http_web 172.25.250.11:80
        mode http
        balance roundrobin  
        option httpchk
        option forwardfor
        server server1 172.25.250.10:80 weight 1 maxconn 512 check
        server server2 172.25.250.13:80 weight 1 maxconn 512 check
        server server3 172.25.250.14:80 weight 1 maxconn 512 check
        server server4 172.25.250.15:80 weight 1 maxconn 512 check
